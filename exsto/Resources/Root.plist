<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>

		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>footerCellClass</key><string>EXSTOHeader</string>
		</dict>

		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Settings</string>
		</dict>

		<dict>
			<key>cell</key><string>PSSwitchCell</string>
			<key>default</key><true/>
			<key>defaults</key><string>com.zachatrocity.exsto</string>
			<key>label</key><string>Show Notification Glow</string>
			<key>key</key><string>EXSTO_SHOW_NOTIF_GLOW</string>
			<key>PostNotification</key><string>com.zachatrocity.exsto/prefsChanged</string>
		</dict>

		<dict>
			<key>cell</key><string>PSSwitchCell</string>
			<key>default</key><false/>
			<key>defaults</key><string>com.zachatrocity.exsto</string>
			<key>label</key><string>Limit Icons</string>
			<key>key</key><string>EXSTO_LIMIT_ICONS</string>
			<key>PostNotification</key><string>com.zachatrocity.exsto/prefsChanged</string>
		</dict>

		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Icon Limit</string>
		</dict>

		<dict>
			<key>cell</key><string>PSSliderCell</string>
			<key>default</key><real>0</real>
			<key>defaults</key><string>com.zachatrocity.exsto</string>
			<key>key</key><string>EXSTO_MAX_ICONS</string>
			<key>min</key><real>0</real>
			<key>max</key><real>100</real>
			<key>showValue</key><true/>
			<key>isSegmented</key><false/>
			<key>PostNotification</key><string>com.zachatrocity.exsto/prefsChanged</string>
		</dict>

		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Opening Delay (ms)</string>
		</dict>

		<dict>
			<key>cell</key><string>PSSliderCell</string>
			<key>default</key><real>0.2</real>
			<key>defaults</key><string>com.zachatrocity.exsto</string>
			<key>key</key><string>EXSTO_DELAY_SPEED</string>
			<key>min</key><real>0</real>
			<key>max</key><real>0.5</real>
			<key>showValue</key><true/>
			<key>isSegmented</key><false/>
			<key>PostNotification</key><string>com.zachatrocity.exsto/prefsChanged</string>
		</dict>

		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Open Radius (Distance From Folder)</string>
		</dict>

		<dict>
			<key>cell</key><string>PSSliderCell</string>
			<key>default</key><real>80</real>
			<key>defaults</key><string>com.zachatrocity.exsto</string>
			<key>key</key><string>EXSTO_RADIUS</string>
			<key>min</key><real>0</real>
			<key>max</key><real>150</real>
			<key>showValue</key><true/>
			<key>isSegmented</key><false/>
			<key>PostNotification</key><string>com.zachatrocity.exsto/prefsChanged</string>
		</dict>

		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Credits</string>
			<key>footerText</key><string>Exsto by Zach Russell. Updates by 6ilent</string>
		</dict>

		<dict>
	    <key>cellClass</key><string>HBTwitterCell</string>
	    <key>label</key><string>Zach Russell</string>
	    <key>user</key><string>zachatrocity</string>
		</dict>

		<dict>
	    <key>cellClass</key><string>HBTwitterCell</string>
	    <key>label</key><string>6ilent</string>
	    <key>user</key><string>zer0entry</string>
		</dict>

		<dict>
			<key>cellClass</key><string>HBLinkTableCell</string>
			<key>label</key><string>Source Code</string>
			<key>subtitle</key><string>Source Code</string>
			<key>url</key><string>https://github.com/6ilent/ExstoXS</string>
		</dict>

	</array>
	<key>title</key>
	<string>Exsto XS</string>
</dict>
</plist>
